# Pa√≠ses bloqueados por defecto
# CA = Canada
# CN = China
# RU = Rusia
# UA = Ucrania
# VN = Vietnam

define CountriesToBlock = { CA, CN, RU, UA, VN }

table netdev filter {

  set non-spanish-speaking-countries-ipv4 {
    type ipv4_addr
    flags interval
    elements = { $CA.ipv4, $CN.ipv4, $RU.ipv4, $UA.ipv4, $VN.ipv4 }
  }

  set non-spanish-speaking-countries-ipv6 {
    type ipv6_addr
    flags interval
    elements = { $CA.ipv6, $CN.ipv6, $RU.ipv6, $UA.ipv6, $VN.ipv6 }
  }
  
  chain ingress {
    type filter hook ingress device eth0 priority 0; policy accept;
     ip saddr @non-spanish-speaking-countries-ipv4 counter drop
    ip6 saddr @non-spanish-speaking-countries-ipv6 counter drop
  }

}
